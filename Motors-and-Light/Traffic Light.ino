void setup()
{
 pinMode(9,OUTPUT); 
  pinMode(10,OUTPUT);
   pinMode(8,OUTPUT);
}
void loop()
{
 digitalWrite(10,255) ;
 tone(3,200,1000);
 delay(5000);
  digitalWrite(10,0) ;
 digitalWrite(9,255);
 tone(3,400,1000);
 delay(5000);
  digitalWrite(9,0);
 digitalWrite(8,255);
 tone(3,200,1000);
 delay(1000);
  digitalWrite(8,0);
}
